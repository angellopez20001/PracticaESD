# DATASET PRINCIPAL


<https://um.es/data/DMDgrafoDataset>
    a dcat:Dataset ;
    dct:title "Grafo de Conocimiento sobre Distrofia Muscular de Duchenne"@es ;
    dct:title "Knowledge Graph on Duchenne Muscular Dystrophy"@en ;
    dct:description """Grafo RDF que integra información genética, molecular, fenotípica y terapéutica 
    sobre la Distrofia Muscular de Duchenne (DMD). Incluye mutaciones del gen DMD, clasificación 
    funcional según Sequence Ontology, fenotipos clínicos anotados con HPO, funciones biológicas 
    de la distrofina según GO, y tratamientos dirigidos incluyendo terapias de exon skipping.
    El grafo utiliza el modelo BioLink para garantizar interoperabilidad."""@es ;
    
    # CREADOR 
    dct:creator <https://um.es/person/AngelLopez> ;
    
    # FECHAS 
    dct:created "2026-01-17"^^xsd:date ;
    dct:modified "2026-01-17"^^xsd:date ;
    dct:issued "2026-01-17"^^xsd:date ;
    
    # TEMAS Y PALABRAS CLAVE
    dcat:theme <http://purl.obolibrary.org/obo/DOID_11723> ;
    dcat:keyword "Distrofia Muscular de Duchenne"@es, "Duchenne Muscular Dystrophy"@en ;
    dcat:keyword "DMD gene"@en, "Gen DMD"@es ;
    dcat:keyword "Distrofina"@es, "Dystrophin"@en ;
    dcat:keyword "Exon skipping"@en ;
    dcat:keyword "BioLink Model"@en ;
    dcat:keyword "Linked Data"@en ;
    
    # LICENCIA
    dct:license <https://opensource.org/licenses/MIT> ;
    cc:license <http://creativecommons.org/licenses/by/4.0/> ;
    
    # IDIOMA
    dct:language <http://id.loc.gov/vocabulary/iso639-1/es> ;
    dct:language <http://id.loc.gov/vocabulary/iso639-1/en> ;
    
    # CONTACTO 
    dcat:contactPoint [
        a vcard:Individual ;
        vcard:fn "AngelLopezFrances" ;
        vcard:hasEmail <mailto:angel.lopezf@um.es>
    ] ;
    
    # LANDING PAGE (CAMBIAR CON TU PUERTO)
    dcat:landingPage <http://dayhoff.inf.um.es:8181/DuchenneMuscularDystrophy> ;
    
    # VERSIÓN
    dcat:version "1.0" ;
    
    # DISTRIBUCIONES
    dcat:distribution <https://um.es/data/DMDgrafo_rdf> ;
    dcat:distribution <https://um.es/data/DMDgrafo_sparql> ;
    
    # PROCEDENCIA
    prov:wasDerivedFrom <https://www.ncbi.nlm.nih.gov/clinvar/> ;
    prov:wasDerivedFrom <https://www.dmd.nl/> ;
    prov:wasDerivedFrom <https://www.drugbank.ca/> ;
    prov:wasDerivedFrom <https://hpo.jax.org/> ;
    
    # VOCABULARIOS UTILIZADOS
    void:vocabulary <http://purl.obolibrary.org/obo/so.owl> ;
    void:vocabulary <http://purl.obolibrary.org/obo/hp.owl> ;
    void:vocabulary <http://purl.obolibrary.org/obo/go.owl> ;
    void:vocabulary <http://purl.obolibrary.org/obo/doid.owl> ;
    void:vocabulary <http://purl.obolibrary.org/obo/ro.owl> ;
    void:vocabulary <https://w3id.org/biolink/biolink-model.owl> ;
    
    # ESTADÍSTICAS 
    void:triples "120"^^xsd:integer ;
    void:entities "35"^^xsd:integer ;
    void:properties "12"^^xsd:integer .


